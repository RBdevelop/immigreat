<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>
  <%= devise_error_messages! %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :twitter_handle %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
